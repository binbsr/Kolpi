<MudPaper Width="250px" Class="d-inline-flex py-3" Elevation="0">
    <MudNavMenu Class="mud-width-full">
        <MudText Typo="Typo.h6" Class="px-4">Material</MudText>
        <MudText Typo="Typo.body2" Class="px-4 mud-text-secondary">Icons</MudText>
        <MudDivider Class="my-2" />
        <MudNavLink Href="/dashboard" Icon="@Icons.Filled.Dashboard">Dashboard</MudNavLink>
        <MudNavLink Href="/users" Icon="@Icons.Filled.Storage">Users</MudNavLink>
        
        <MudNavGroup Title="Questions" Icon="@Icons.Filled.QuestionMark" Expanded="true">
                <MudNavLink Href="/question/index" Icon="@Icons.Filled.TableRows" IconColor="Color.Info">All Questions</MudNavLink>
                <MudNavLink Href="/question/add" Icon="@Icons.Filled.Add" IconColor="Color.Info">Add New</MudNavLink>
        </MudNavGroup>

        <MudNavGroup Title="Admin" Icon="@Icons.Filled.AdminPanelSettings" Expanded="false">
            <MudNavGroup Title="Tagging" Icon="@Icons.Filled.Tag" Expanded="true">
                <MudNavLink Href="/tags/index" Icon="@Icons.Filled.TableRows" IconColor="Color.Info">All Tags</MudNavLink>
                <MudNavLink Href="/tags/add" Icon="@Icons.Filled.Add" IconColor="Color.Info">Add New</MudNavLink>
            </MudNavGroup>
            <MudNavGroup Title="Tag Categories" Icon="@Icons.Filled.Category" Expanded="true">
                <MudNavLink Href="/tagtypes/index" Icon="@Icons.Filled.TableRows" IconColor="Color.Info">All Categories</MudNavLink>
                <MudNavLink Href="/tagtypes/add" Icon="@Icons.Filled.Add" IconColor="Color.Info">Add New</MudNavLink>
            </MudNavGroup>                
        </MudNavGroup>
        
    </MudNavMenu>
</MudPaper>
